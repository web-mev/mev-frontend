{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./projects/web-mev/src",
    "paths": {
      "@app/*": ["app/*"],
      "@core/*": ["app/core/*"],
      "@environments/*": ["environments/*"],
      "@features/*": ["app/features/*"],
      "@file-manager/*": ["app/features/file-manager/*"],
      "@workspace-manager/*": ["app/features/workspace-manager/*"]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "esnext",
    // "target": "es2015",
    "target": "es2020",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom", "dom.iterable"],
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
